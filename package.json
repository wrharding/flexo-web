    "@storybook/react": "^6.1.20",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.20.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
